{
  "$schema": "./node_modules/@stryker-mutator/core/schema/stryker-schema.json",
  "mutator": "javascript",
  "packageManager": "npm",
  "reporters": [
    "progress", "clear-text"
  ],
  "mutate": [
    "core/**/*.js",
    "!web/**/*",
    "!web/App.js"
  ],
  "testRunner": "command",
  "transpilers": [
    "babel"
  ],
  "coverageAnalysis": "off",
  "babel": {
    "optionsFile": ".babelrc"
  }
}
